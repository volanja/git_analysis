<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Git Analysis" %></title>
    
    <%= stylesheet_link_tag "http://volanja.github.io/git_analysis/stylesheets/all.css" %>
    <%= stylesheet_link_tag "http://volanja.github.io/git_analysis/stylesheets/normalize.css" %>
    <%= javascript_include_tag  "http://volanja.github.io/git_analysis/javascripts/all.js" %>
  </head>
  
  <body class="<%= page_classes %>">
    <%= yield %>
  </body>
</html>
